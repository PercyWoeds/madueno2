<p id="notice"><%= notice %></p>

<h1>Listing Tipoavisos</h1>

<table>
  <thead>
    <tr>
      <th>Descrip</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipoavisos.each do |tipoaviso| %>
      <tr>
        <td><%= tipoaviso.descrip %></td>
        <td><%= tipoaviso.comments %></td>
        <td><%= link_to 'Show', tipoaviso %></td>
        <td><%= link_to 'Edit', edit_tipoaviso_path(tipoaviso) %></td>
         <%     if current_user.level != "admin"  %>

        <td><%= link_to 'Destroy', tipoaviso, method: :delete, data: { confirm: 'Are you sure?' } %></td>

        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>


<br>

<%= link_to 'Nuevo Programa/ Elemento', new_tipoaviso_path %>


<h2>Importar Archivo clientes "*.csv"</h2>

<%= form_tag import_tipoavisos_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Importar " %>
<% end %>

