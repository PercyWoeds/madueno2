<p id="notice"><%= notice %></p>

<h1>Listing Tipo Tarifas</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_tarifas.each do |tipo_tarifa| %>
      <tr>
        <td><%= tipo_tarifa.code %></td>
        <td><%= tipo_tarifa.name %></td>
        <td><%= tipo_tarifa.user_id %></td>
        <td><%= link_to 'Show', tipo_tarifa %></td>
        <td><%= link_to 'Edit', edit_tipo_tarifa_path(tipo_tarifa) %></td>
        <td><%= link_to 'Destroy', tipo_tarifa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo tarifa', new_tipo_tarifa_path %>
