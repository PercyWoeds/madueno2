<%= simple_form_for(@medio_detail) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :code %>
    <%= f.input :name %>
    <%= f.input :user_id %>
    <%= f.input :medio_id %>
    <%= f.input :references %>
    <%= f.input : %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
