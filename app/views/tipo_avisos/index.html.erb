<p id="notice"><%= notice %></p>

<h1>Listing Tipo Avisos</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_avisos.each do |tipo_aviso| %>
      <tr>
        <td><%= tipo_aviso.code %></td>
        <td><%= tipo_aviso.name %></td>
        
        <td><%= link_to 'Show', tipo_aviso %></td>
        <td><%= link_to 'Edit', edit_tipo_aviso_path(tipo_aviso) %></td>
        <td><%= link_to 'Destroy', tipo_aviso, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo aviso', new_tipo_aviso_path %>
