<p id="notice"><%= notice %></p>

<h1>Listing Tipo Cpms</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_cpms.each do |tipo_cpm| %>
      <tr>
        <td><%= tipo_cpm.code %></td>
        <td><%= tipo_cpm.name %></td>
        <td><%= link_to 'Show', tipo_cpm %></td>
        <td><%= link_to 'Edit', edit_tipo_cpm_path(tipo_cpm) %></td>
        <td><%= link_to 'Destroy', tipo_cpm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo cpm', new_tipo_cpm_path %>
