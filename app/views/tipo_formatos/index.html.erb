<p id="notice"><%= notice %></p>

<h1>Listing Tipo Formatos</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_formatos.each do |tipo_formato| %>
      <tr>
        <td><%= tipo_formato.code %></td>
        <td><%= tipo_formato.name %></td>
        <td><%= link_to 'Show', tipo_formato %></td>
        <td><%= link_to 'Edit', edit_tipo_formato_path(tipo_formato) %></td>
        <td><%= link_to 'Destroy', tipo_formato, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo formato', new_tipo_formato_path %>
